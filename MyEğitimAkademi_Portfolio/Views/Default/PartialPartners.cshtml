@model List<MyEğitimAkademi_Portfolio.Models.Partner>
<section class="w3l-homeblock3" id="clients">
    <div class="midd-w3 py-5">
        <div class="container py-lg-5 py-md-3">
            <div class="row">
                <div class="col-lg-6">
                    <span class="text">Müşterilerim ve Ortaklarım</span>
                    <h3 class="title-big">
                        Müşteri / Ortaklar
                        <span> İşbirliği.</span>
                    </h3>
                    <p class="mt-4">
                        Over the past ten years, I've been fortunate to work with and for people from some
                        amazing organizations.
                    </p>
                    <div class="reward-list mt-5">
                        @foreach (var item in Model)
                        {
                            <div class="reward-item">
                                <div class="reward-item-photo">
                                    @item.Value
                                </div>
                                <div class="reward-item-detail">
                                    <span class="reward-item-title">@item.Title</span>
                                    <p class="reward-item-descr">@item.Description</p>
                                </div>
                            </div>
                        }


                    </div>
                </div>
                <div class="col-lg-6 align-self text-center mt-lg-0 mt-md-5 mt-4">
                    <div class="images">
                        <img src="~/Templates/iportfolio-starter/assets/images/client1.png" class="img-fluid" alt="" />
                        <img src="~/Templates/iportfolio-starter/assets/images/client2.png" class="img-fluid" alt="" />
                        <img src="~/Templates/iportfolio-starter/assets/images/client3.png" class="img-fluid" alt="" />
                        <img src="~/Templates/iportfolio-starter/assets/images/client4.png" class="img-fluid" alt="" />
                        <img src="~/Templates/iportfolio-starter/assets/images/client5.png" class="img-fluid" alt="" />
                        <img src="~/Templates/iportfolio-starter/assets/images/client6.png" class="img-fluid" alt="" />
                        <img src="~/Templates/iportfolio-starter/assets/images/client2.png" class="img-fluid" alt="" />
                        <img src="~/Templates/iportfolio-starter/assets/images/client3.png" class="img-fluid" alt="" />
                        <img src="~/Templates/iportfolio-starter/assets/images/client4.png" class="img-fluid" alt="" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>